{
  "name": "quickdrop",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon app.js",
    "server": "concurrently \"nodemon app.js\" \"npm run client\"",
    "client": "npm start --prefix client",
    "dev": "concurrently \"node app.js\" \"npm run client\" ",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "dependencies": {
    "concurrently": "^3.6.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "fs": "0.0.1-security",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "morgan": "~1.9.0",
    "nodemon": "^1.17.5",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-dropzone": "^4.2.13",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "superagent": "^3.8.3"
  }
}
